    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MineSweeperUWP.View"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Page.Background>
        <AcrylicBrush TintColor="#CC373737" />
    </Page.Background>
    <Grid>
        <TextBlock x:Name="textBlock" Text="LeaderBoard" TextWrapping="Wrap" FocusVisualPrimaryBrush="#FFF7F6F6"  FontFamily="Century Gothic" FontWeight="Bold" FontSize="20
                   " Margin="116,107,0,0" Foreground="#FFF3F3F3" Height="36" VerticalAlignment="Top" HorizontalAlignment="Left" Width="139">
        </TextBlock>
        <ListBox x:Name="listBox" Margin="116,237,155,180" />
        <Button Style="{StaticResource NavigationBackButtonNormalStyle}" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0,0,1460,960" />
    </Grid>
</Page>